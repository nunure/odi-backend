{
  "name": "odi-backend",
  "version": "0.0.0",
  "main": "dist",
  "author": "Mathieu RETY <rety.mathieu@gmail.com>",
  "private": true,
  "engines": {
    "node": "10.11.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "preserve": "npm run -s build",
    "serve": "cross-env NODE_ENV=production node dist",
    "lint": "eslint ."
  },
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "body-parser": "~1.18.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "docxtemplater": "^3.9.2",
    "dotenv": "^6.1.0",
    "express": "~4.16.4",
    "helmet": "^3.14.0",
    "http-status": "^1.2.0",
    "if-env": "^1.0.4",
    "jszip": "^2.6.1",
    "mongoose": "^5.3.9",
    "morgan": "~1.9.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.9.0",
    "nodemon": "^1.18.6"
  }
}
